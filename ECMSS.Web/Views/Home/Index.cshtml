<div class="areaC">
    <a class="btn_area" id="btn_area"> <img src="~/assets/imgs/btn_areaL.png" /></a>
    <div class="areacBox">
        <div class="treeFolder">
            <div class="baseFolder">
                <div class="txt">
                    <p>Shortcut Box>My Content</p>
                </div>
            </div>
        </div>
        <div class="listContent">
            <div class="left">
                <div class="bt_resign">
                    <a onclick="addnew()" class="btn_add">
                        <span class="add_new"> Add New File</span>
                    </a>
                    <a onclick="createnew()" class="btn_create">
                        <span class="create_new">Create New Content</span>
                    </a>
                    <a onclick="deleteFile()" class="btn-delete-file btn-warning" id="btn-delete-file">
                        <span>Delete File</span>
                    </a>
                    <a onclick="deleteFolder()" class="btn-delete-folder btn-danger hidelement" id="btn-delete-folder">
                        <span>Delete Folder</span>
                    </a>
                </div>
            </div>
            <div class="right">
                <div class="listSearch">
                    <input id="txtSearch" type="text" placeholder="File name/owner ..." name="name" />
                    <input type="image" class="btnListSearch" onclick="searchContent()" src="~/assets/imgs/btn_search.gif" />
                </div>
            </div>
        </div>
        <div class="contentList">
            <ul class="listicon">
                <li><a onclick="filterFile()"><img src="~/assets/imgs/ico_all_on.png" alt="All File" /></a> </li>
                <li><a onclick="filterFile('powerpoint')"><img src="~/assets/imgs/ico_ppt_on.png" alt="Powerpoint" /></a> </li>
                <li><a onclick="filterFile('excel')"><img src="~/assets/imgs/ico_xlsx_on.png" alt="Excel" /></a> </li>
                <li><a onclick="filterFile('word')"><img src="~/assets/imgs/ico_doc_on.png" alt="Word" /></a> </li>
                <li><a onclick="filterFile('pdf')"><img src="~/assets/imgs/ico_pdf_on.png" alt="Pdf" /></a> </li>
                <li><a onclick="filterFile('image')"><img src="~/assets/imgs/ico_img_on.png" alt="Image" /></a> </li>
            </ul>
            <table id="tbMainDefault" class="table table-striped table-bordered post-list-table">
                <thead>
                    <tr class="tbheader" style="background-color: rgba(223, 225, 228, 1)">
                        <th class="text-center" style="width: 35%"><span>File name</span></th>
                        <th>owner</th>
                        <th>Modifier</th>
                        <th>Size</th>
                        <th>Security level</th>
                        <th>Version</th>
                        <th>Modified date</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>